  "scripts": {
    "build": "tsc",
    "start": "cross-env NODE_ENV=production node dist/index.js",
    "dev": "cross-env NODE_ENV=development ts-node-dev --respawn --transpile-only src/index.ts",
    "test": "echo \"Error: no test specified\" && exit 1",
    "lint": "eslint 'src/**/*.{js,ts}'",
    "pm2:start": "pm2 start dist/index.js --name server",
    "pm2:stop": "pm2 stop server",
    "pm2:restart": "pm2 restart server",
    "clean": "rm -rf dist",
    "build:prod": "npm run clean && cross-env NODE_ENV=production tsc",
    "db:migrate": "prisma migrate dev",
    "db:deploy": "prisma migrate deploy",
    "db:reset": "prisma migrate reset",
    "db:generate": "prisma generate",
    "db:studio": "prisma studio",
    "db:seed": "ts-node prisma/seed.ts",
    "test:image-processing": "ts-node src/test-api.ts"
  },
